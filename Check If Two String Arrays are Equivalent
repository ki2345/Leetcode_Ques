// LeetCode 1662
// Check If Two String Arrays are Equivalent -- Easy
class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        String str1 = "";
        for(int i=0;i<word1.length;i++){
            str1 += word1[i];
        }
        String str2 = "";
        for(int i=0;i<word2.length;i++){
            str2 += word2[i];
        }
        if(str1.equals(str2))
            return true;
        return false;
    }
}
